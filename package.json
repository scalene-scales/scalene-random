{
  "name": "@scalene-scales/scalene-random",
  "version": "2.0.3",
  "description": "A determistic PRNG library.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "clean": "rm -rf ./dist",
    "test": "jest --config jest.config.json",
    "test:coverage": "jest --config jest.config.json --coverage",
    "lint": "eslint src --ext js,jsx,ts,tsx --cache",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "pre-commit": [
    "lint",
    "test",
    "clean",
    "build"
  ],
  "keywords": [],
  "author": "Scalene Scales",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.4.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-jest": "^29.4.2",
    "eslint": "^8.34.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.2",
    "pre-commit": "^1.2.2",
    "ts-essentials": "^9.3.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scalene-scales/scalene-random.git"
  },
  "dependencies": {
    "@scalene-scales/scalene-binary": "^1.0.1",
    "uuid": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/scalene-scales/scalene-random/issues"
  },
  "homepage": "https://github.com/scalene-scales/scalene-random#readme"
}
